<div id="account-management">
  <% if (title) { %>
    <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>
  <%- messages() %>
  <% if (errors?.array().filter(error => error.msg !== 'Invalid value').length > 0) { %>
    <ul class="notice">
      <% errors.array().filter(error => error.msg !== 'Invalid value').forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } else { %>
    <p>You're logged in</p>
  <% } %>
</div>